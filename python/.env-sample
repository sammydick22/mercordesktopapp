# Rename this file to .env to use these variables in test scripts

# Supabase Test Credentials
# These are used by the task sync verification script when testing without supplying manual credentials
SUPABASE_TEST_EMAIL=your-email@example.com
SUPABASE_TEST_PASSWORD=your-password

# Supabase Project Configuration
# These should match your Supabase project settings
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-anon-key

# Cerebras API key for fake work detection
CEREBRAS_API_KEY=your-cerebras-api-key-here

# Auth token for testing local API endpoints
AUTH_TOKEN=your-authentication-token-here
